{
  "name": "@test-pod/database",
  "version": "1.0.0",
  "description": "Modelos de dados usando Sequelize",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "export NODE_ENV=test && jest",
    "test:watch": "export NODE_ENV=test && jest --watch",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:down": "sequelize-cli db:migrate:undo",
    "migrate:down:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "seed:all": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "@test-pod/jest-config": "workspace:*",
    "@test-pod/ts-config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "better-sqlite3": "^12.1.1",
    "jest": "^30.0.3",
    "jest-environment-node": "^30.0.2",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}