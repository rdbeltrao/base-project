{
  "name": "@test-pod/database",
  "version": "1.0.0",
  "description": "Modelos de dados usando Sequelize",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest -i",
    "test:watch": "jest --watch",
    "migrate:generate": "export NODE_ENV=development && sequelize-cli migration:generate --name",
    "migrate:up": "export NODE_ENV=development && sequelize-cli db:migrate",
    "migrate:down": "export NODE_ENV=development && sequelize-cli db:migrate:undo",
    "migrate:down:all": "export NODE_ENV=development && sequelize-cli db:migrate:undo:all",
    "migrate:status": "export NODE_ENV=development && sequelize-cli db:migrate:status",
    "seed:all": "export NODE_ENV=development && sequelize-cli db:seed:all",
    "seed:undo": "export NODE_ENV=development && sequelize-cli db:seed:undo",
    "seed:undo:all": "export NODE_ENV=development && sequelize-cli db:seed:undo:all",
    "seed:run": "export NODE_ENV=development && sequelize-cli db:seed --seed",
    "seed:reset": "export NODE_ENV=development && sequelize-cli db:seed:undo:all && sequelize-cli db:seed:all",
    "db:reset": "export NODE_ENV=development && sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "@test-pod/eslint-config": "workspace:*",
    "@test-pod/prettier-config": "workspace:*",
    "@test-pod/ts-config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.29.0",
    "jest": "^30.0.3",
    "jest-environment-node": "^30.0.2",
    "prettier": "^3.6.1",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.2"
  }
}
